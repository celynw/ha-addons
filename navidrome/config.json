		"ND_LASTFM_APIKEY": "",
		"ND_LASTFM_SECRET": "",
		"ND_JUKEBOX_ENABLED": false,
		"ND_JUKEBOX_DEFAULT": "",
		"ND_JUKEBOX_DEVICES": "",
