		"ND_MUSICFOLDER": null,
		"ND_DATAFOLDER": "/config/addons_config/navidrome",
		"ND_ENABLESTARRATING": true,
		"ND_ENABLEFAVOURITES": true,
		"ND_LISTENBRAINZ_ENABLED": true,
