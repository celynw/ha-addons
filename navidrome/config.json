{
	"arch": [
		"aarch64",
		"amd64",
		"armv7"
	],
	"backup_exclude": [
		"**/cache"
	],
	"description": "Modern Music Server and Streamer compatible with Subsonic/Airsonic",
	"devices": [
		"/dev/dri",
		"/dev/dri/card0",
		"/dev/dri/card1",
		"/dev/dri/renderD128",
		"/dev/vchiq",
		"/dev/video10",
		"/dev/video11",
		"/dev/video12",
		"/dev/video13",
		"/dev/video14",
		"/dev/video15",
		"/dev/video16",
		"/dev/ttyUSB0",
		"/dev/sda",
		"/dev/sdb",
		"/dev/sdc",
		"/dev/sdd",
		"/dev/sde",
		"/dev/sdf",
		"/dev/sdg",
		"/dev/nvme",
		"/dev/nvme0n1p1",
		"/dev/nvme0n1p2",
		"/dev/mmcblk",
		"/dev/fuse",
		"/dev/sda1",
		"/dev/sdb1",
		"/dev/sdc1",
		"/dev/sdd1",
		"/dev/sde1",
		"/dev/sdf1",
		"/dev/sdg1",
		"/dev/sda2",
		"/dev/sdb2",
		"/dev/sdc2",
		"/dev/sdd2",
		"/dev/sde2",
		"/dev/sdf2",
		"/dev/sdg2",
		"/dev/sda3",
		"/dev/sdb3",
		"/dev/sda4",
		"/dev/sdb4",
		"/dev/sda5",
		"/dev/sda6",
		"/dev/sda7",
		"/dev/sda8",
		"/dev/nvme0",
		"/dev/nvme1",
		"/dev/nvme2"
	],
	"ingress": true,
	"ingress_port": 4533,
	"init": false,
	"map": [
		"media:rw",
		"config:rw",
		"share:rw"
	],
	"name": "Navidrome",
	"options": {
		"ND_DATAFOLDER": "/config/addons_config/navidrome",
		"ND_ENABLESHARING": true,
		"ND_LOGLEVEL": "info",
		"ND_MUSICFOLDER": "/media/Media/music",
		"ND_SCANINTERVAL": "1m",
		"ND_SESSIONTIMEOUT": "30m",
		"ND_TRANSCODINGCACHESIZE": "100MB",
		"PGID": 0,
		"PUID": 0
	},
	"panel_icon": "mdi:record-circle-outline",
	"ports": {
		"4533/tcp": 4533
	},
	"ports_description": {
		"4533/tcp": "WebUI"
	},
	"privileged": [
		"SYS_TIME"
	],
	"schema": {
		"ND_DATAFOLDER": "str?",
		"ND_ENABLESHARING": "bool",
		"ND_LOGLEVEL": "str?",
		"ND_MUSICFOLDER": "str?",
		"ND_SCANINTERVAL": "str?",
		"ND_SESSIONTIMEOUT": "str?",
		"ND_TRANSCODINGCACHESIZE": "str",
		"PGID": "int",
		"PUID": "int"
	},
	"slug": "navidrome",
	"url": "https://github.com/celynw/ha-addons/tree/main/navidrome",
	"version": "0.51.1",
	"webui": "http://[HOST]:[PORT:4533]"
}
